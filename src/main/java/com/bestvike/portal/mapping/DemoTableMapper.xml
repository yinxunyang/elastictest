<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bestvike.portal.dao.DemoTableDao">
    <select id="pageDemoTable" parameterType="Map" resultType="DemoTable">
        <![CDATA[
			select * from demo_table
		]]>
        <where>
            <![CDATA[
            sys_guid is not null
            ]]>
            <include refid="com.bestvike.commons.dao.AbstractDao.query"/>
        </where>
        <include refid="com.bestvike.commons.dao.AbstractDao.order"/>
    </select>
</mapper>