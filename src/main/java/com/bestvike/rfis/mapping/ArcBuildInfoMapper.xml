<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bestvike.rfis.dao.ArcBuildInfoDao">
    <select id="getArcBuildInfo"  resultType="ArcBuildInfo">
        <![CDATA[
			select * from Arc_BuildInfo
		]]>
    </select>

    <select id="queryArcBuildInfoBybldNo" parameterType="String"  resultType="ArcBuildInfo">
        <![CDATA[
			select * from Arc_BuildInfo where bldNo = #{bldNo}
		]]>
    </select>


</mapper>
